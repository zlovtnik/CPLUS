{
  "server": {
    "address": "0.0.0.0",
    "port": "${PORT:-8080}",
    "threads": 2
  },
  "database": {
    "host": "${DATABASE_HOST:-localhost}",
    "port": "${DATABASE_PORT:-5432}",
    "name": "${DATABASE_NAME:-etlplus}",
    "username": "${DATABASE_USERNAME:-etlplus}",
    "password": "${DATABASE_PASSWORD:-etlplus123}",
    "type": "postgresql"
  },
  "auth": {
    "jwt_secret": "${JWT_SECRET:-your-secret-key-here}",
    "session_timeout": 3600
  },
  "etl": {
    "max_concurrent_jobs": 2,
    "job_timeout": 1800
  }
}
